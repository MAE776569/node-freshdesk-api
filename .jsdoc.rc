{
	"source": {
		"include": [
			"./lib"
		]
	},
	"plugins": [
		"plugins/markdown",
		"plugins/summarize"
	],
	"opts": {
		"destination": "./docs/",
		"recurse": true,
		"readme": "./README.md"
	},
	"markdown": {
		"idInHeadings": true
	},
	"templates": {
		"default": {
			"staticFiles": {
				"include": [
					"./docs/custom.css" // file position
				]
			}
		}
  	}
}
